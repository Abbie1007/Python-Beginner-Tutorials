   "source": [
    "import numpy as np\n",
    "\n",
    "dx=float(input('please input dx='))\n",
    "area=0\n",
    "y=0\n",
    "for x in np.arange(2,5,dx):\n",
    "    y=(x**2)+1\n",
    "    area=area+(y*dx)\n",
    "print('area=', area)"
