    "import sympy as sp\n",
    "import numdifftools as nd\n",
    "\n",
    "x = sp.Symbol('x')\n",
    "print(sp.diff(3*x**2+1, x))\n",
    "\n",
    "\n",
    "def f(x):\n",
    "    return 3* x**2+1\n",
    "\n",
